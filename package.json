{
  "name": "game_back_end",
  "version": "1.0.0",
  "description": "use node socket build the game back-end",
  "main": "index.js",
  "scripts": {
    "start": "node -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.141",
    "@types/mongoose": "^5.10.0",
    "@types/node": "^12.7.8",
    "@types/socket.io": "^2.1.3",
    "@types/uuid": "^3.4.5",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.2",
    "socket.io": "^2.3.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "uuid": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinch-top/websocket-node.git"
  },
  "author": "robinch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/robinch-top/websocket-node/issues"
  },
  "homepage": "https://github.com/robinch-top/websocket-node#readme"
}
